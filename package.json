{
  "name": "express-prisma-boilerplate",
  "version": "0.1.0",
  "description": "My boilerplate for building REST APIs with Express, Prisma, and Cloudinary in TypeScript",
  "main": "./src/server.ts",
  "prisma.schema": "./src/prisma/schema.prisma",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "prisma:migrate": "prisma migrate dev",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "prisma:seed": "prisma db seed --preview-feature",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ExpressJS",
    "Prisma",
    "TypeScript",
    "Cloudinary",
    "REST"
  ],
  "author": "Tawanda Munongo",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.12.0",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "nodemon": "^2.0.22"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "prisma": "^4.12.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
